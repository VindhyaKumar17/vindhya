def linear_search(arr, key):
    for i in range(len(arr)):
        if arr[i] == key:
            return i
    return -1
arr = [10, 25, 30, 45, 50]
key=int(input ("Enter the key to be searched:"))
result = linear_search(arr,key)
if result != -1:
    print(f"{key} is found at index {result}")
else:
    print(f"{key} is not found in the list")